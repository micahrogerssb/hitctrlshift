---
interface Props {
  title: string;
  description: string;
}

const { title, description } = Astro.props;
---

<div class="text-center mb-12">
  <h1 class="text-3xl font-sans font-black text-white/90 mb-4">{title}</h1>
  <p class="text-lg text-white/70 font-serif max-w-2xl mx-auto">{description}</p>
</div>